<head>
  <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false&libraries=places"></script>
  <script>
      function initialize() {
          new google.maps.places.Autocomplete(document.getElementById('searchTextFieldFrom'));
          new google.maps.places.Autocomplete(document.getElementById('searchTextFieldTo'));
      }
      google.maps.event.addDomListener(window, 'load', initialize);
  </script>
</head>
<div>
  <label for="searchTextFieldFrom">From:</label><input id="searchTextFieldFrom" type="text" size="500">
</div>
<div>
  <label for="searchTextFieldTo">To:</label><input id="searchTextFieldTo" type="text" size="500">
</div>
